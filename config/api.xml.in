<object>
  <scalar name="DBI_HOST">@db_host@;mysql_ssl=1</scalar>
  <scalar name="DBI_DSN">dbi:mysql</scalar>
  <scalar name="DBI_USER">@db_user@</scalar>
  <scalar name="DBI_PASS">@db_password@</scalar>
  <scalar name="DBI_DB">@db_name@</scalar>
  <scalar name="SESSION_DIR">/var/www/bedrock/session</scalar>
  <scalar name="autoconnect">0</scalar>
  <scalar name="use_sessions">1</scalar>
  <scalar name="API_KEY">@api_key@</scalar>
  <scalar name="mysql_ssl">1</scalar>
  <scalar name="base_url">/@api_name@</scalar>
  <scalar name="class">@api_class@</scalar>
  <object name="routes">
    <array name="version">
      <scalar>GET</scalar>
    </array>
    <array name="session">
      <scalar>GET</scalar>
    </array>
    <array name="env">
      <scalar>GET</scalar>
    </array>
    <array name="csp_violation_report">
      <scalar>POST</scalar>
    </array>
  </object>
  <array name="protected">
    <scalar>env</scalar>
  </array>
</object>
